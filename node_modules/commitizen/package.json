    "chalk": "1.1.1",
    "cz-conventional-changelog": "1.1.5",
    "dedent": "0.6.0",
    "detect-indent": "4.0.0",
    "find-node-modules": "1.0.1",
    "find-root": "^1.0.0",
    "glob": "7.0.3",
    "gulp": "3.9.1",
    "gulp-git": "1.7.0",
    "home-or-tmp": "2.0.0",
    "inquirer": "0.12.0",
    "lodash": "4.6.1",
    "minimist": "1.2.0",
    "shelljs": "0.5.3",
    "strip-json-comments": "2.0.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
